{
  "name": "ai-product-factory",
  "version": "2.8.1",
  "private": true,
  "description": "AI Product Factory - Multi-agent workflow system for generating Product Vision and Architecture documents",
  "scripts": {
    "sync-workflows": "node scripts/sync-workflows.js",
    "sync-workflows:dry-run": "node scripts/sync-workflows.js --dry-run",
    "sync-workflows:verbose": "node scripts/sync-workflows.js --verbose",
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm run start",
    "lint": "cd frontend && npm run lint",
    "typecheck": "cd frontend && npm run typecheck",
    "test": "vitest run tests/",
    "test:watch": "vitest tests/",
    "test:coverage": "vitest run --coverage",
    "test:backend": "vitest run tests/backend.test.ts",
    "test:frontend": "cd frontend && npm run test",
    "test:sync-dry-run": "bash scripts/test-sync-dry-run.sh",
    "test:migration": "bash scripts/test-migration-idempotency.sh",
    "test:env:up": "docker compose -f docker-compose.test.yml up -d",
    "test:env:down": "docker compose -f docker-compose.test.yml down -v",
    "test:env:logs": "docker compose -f docker-compose.test.yml logs -f",
    "test:env:status": "docker compose -f docker-compose.test.yml ps",
    "test:all": "bash scripts/run-tests.sh",
    "test:integration": "npm run test:env:up && sleep 15 && npm run test:backend && npm run test:env:down"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/curosr-n8n.git"
  },
  "keywords": [
    "n8n",
    "workflow",
    "ai",
    "product-factory",
    "document-generation"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.682.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^22.8.4",
    "@vitejs/plugin-react": "^4.3.3",
    "jsdom": "^25.0.0",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.0",
    "vitest": "^2.1.0"
  }
}
